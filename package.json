{
  "name": "notif-api",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "postinstall": "tsc",
    "start": "TS_NODE_BASEURL=./dist node -r tsconfig-paths/register dist/app.js",
    "dev": "NODE_ENV=development nodemon --config nodemon.json --exec npm run serve",
    "serve": "ts-node -r tsconfig-paths/register src/app.ts",
    "seed": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed",
    "seed:prod": "TS_NODE_BASEURL=./dist node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed",
    "migrate_gen:prod": "TS_NODE_BASEURL=./dist node -r tsconfig-paths/register ./node_modules/typeorm/cli -f ./ormconfig.js migration:generate -n migration",
    "migrate_run:prod": "TS_NODE_BASEURL=./dist node -r tsconfig-paths/register ./node_modules/typeorm/cli -f ./ormconfig.js migration:run",
    "migrate_revert:prod": "TS_NODE_BASEURL=./dist node -r tsconfig-paths/register ./node_modules/typeorm/cli -f ./ormconfig.js migration:revert"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@line/bot-sdk": "^7.5.0",
    "apollo-server-express": "^3.6.7",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dayjs": "^1.11.2",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "firebase-admin": "^10.2.0",
    "graphql": "^15.3.0",
    "module-alias": "^2.2.2",
    "mysql2": "^2.3.3",
    "nanoid": "^3.3.4",
    "node-cron": "^3.0.0",
    "numeral": "^2.0.6",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "type-graphql": "^1.1.1",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.45",
    "typeorm-seeding": "^1.6.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/node-cron": "^3.0.1",
    "@types/numeral": "^2.0.2",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.14.0",
    "eslint-plugin-type-graphql": "^1.0.0",
    "nodemon": "^2.0.16",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4"
  },
  "_moduleAliases": {
    "@": "dist",
    "@graphql": "dist/graphql",
    "@entity": "dist/graphql/entity"
  }
}
